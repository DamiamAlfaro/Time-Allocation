{
  "redirects": [
    {
      "source": "/webapp",
      "destination": "/webapp/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/webapp/",
      "has": [
        {
          "type": "cookie",
          "key": "auth",
          "value": "true"
        }
      ],
      "destination": "/webapp/index.html"
    },
    {
      "source": "/webapp/",
      "destination": "/"
    }
  ]
}
